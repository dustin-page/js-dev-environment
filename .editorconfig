# EditorConfig is awesome: http://EditorConfig.org

## Check if your IDE has native support or requires a plugin to format files based on the .editorconfig file: http://editorconfig.org/#download

# EditorConfig ensures code consistency

# top-most EditorConfig file
root = true

# Indentation override for all JS in project
[*]
indent_style = tab
end_of_line = crlf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false
